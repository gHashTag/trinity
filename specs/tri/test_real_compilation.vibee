name: test_real_compilation
version: "1.0.0"
language: zig
module: test_real_compilation

description: |
  TRUE COMPILATION DEMONSTRATION
  This spec tests if vibee gen generates pub fn from implementation

behaviors:
  - name: add_numbers
    given: Int, Int
    when: Addition operation
    then: Int
    description: Adds two numbers
    implementation: |
      const a = 10;
      const b = 20;
      return a + b;

  - name: multiply_numbers
    given: Int, Int
    when: Multiplication operation
    then: Int
    description: Multiplies two numbers
    implementation: |
      const x = 3;
      const y = 4;
      return x * y;

constants:
  - PHI: 1.618033988749895
  - TRINITY: 3.0

types: []
